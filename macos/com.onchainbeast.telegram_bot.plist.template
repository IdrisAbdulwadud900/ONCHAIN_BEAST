<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.onchainbeast.telegram_bot</string>

  <key>ProgramArguments</key>
  <array>
    <string>__PROJECT_DIR__/macos/run_telegram_bot.sh</string>
  </array>

  <key>WorkingDirectory</key>
  <string>__PROJECT_DIR__</string>

  <key>RunAtLoad</key>
  <true/>

  <key>KeepAlive</key>
  <true/>

  <key>StandardOutPath</key>
  <string>__LOG_DIR__/onchainbeast_telegram_bot.out.log</string>
  <key>StandardErrorPath</key>
  <string>__LOG_DIR__/onchainbeast_telegram_bot.err.log</string>
</dict>
</plist>
